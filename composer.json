{
    "name": "whitespace-se/wordpress-bundle-standard",
    "description": "Bundled plugins for a standard setup of Wordpress",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Whitespace Dev",
            "email": "code@whitespace.se"
        }
    ],
    "type": "metapackage",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": { 
        "php": ">=7.1",
        "composer/installers": "^1.8",
        "hellonico/timber-dump-extension": "^1.0",
        "oscarotero/env": "^2.1",
        "roots/bedrock-autoloader": "^1.0",
        "roots/wp-config": "1.0.0",
        "roots/wp-password-bcrypt": "1.0.0",
        "vlucas/phpdotenv": "^5.2",
        "wpackagist-plugin/disable-comments": "^1.10.0",
        "wpackagist-plugin/disable-gutenberg": "2.2",
        "wpackagist-plugin/disable-wordpress-updates": "^1.6.6",
        "wpackagist-plugin/redirection": "^4.8",
        "wpackagist-plugin/relevanssi":"4.7.2.1",
        "wpackagist-plugin/timber-library": "^1.17",
        "wpackagist-plugin/two-factor-auth":"^4.4",
        "wpackagist-plugin/user-switching": "^1.5",
        "wpackagist-plugin/w3-total-cache": "0.14.3",
        "wpackagist-plugin/wordpress-importer": "^0.7.0",
        "wpackagist-plugin/wordpress-seo": "^14.7",
        "wpackagist-plugin/wp-mail-smtp": "^2.2.1",
        "wpackagist-plugin/wp-nested-pages": "^3.1"
    }
}
